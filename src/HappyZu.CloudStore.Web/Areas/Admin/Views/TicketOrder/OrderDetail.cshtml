@using HappyZu.CloudStore.Trip.Dto

<table>
    <tr>
        <th>编号</th>
        <th>门票编号</th>
        <th>订单编号</th>
        <th>数量</th>
        <th>单价</th>
        <th>总价</th>
    </tr>

    @foreach (var item in ViewBag.Items)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.TicketId</td>
            <td>@item.TicketOrderId</td>
            <td>@item.Quantity</td>
            <td>@item.UnitPrice</td>
            <td>@item.Price</td>
        </tr>
    }
</table>